{
  "name": "vue-vite",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "version": "0.1.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "commit": "czg"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "element-plus": "^2.7.5",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@iconify-json/carbon": "^1.1.36",
    "@types/node": "^20.14.6",
    "@unocss/eslint-config": "^0.53.6",
    "@unocss/reset": "^0.61.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/ui": "^0.34.7",
    "@vue-macros/volar": "^0.10.2",
    "@vue/test-utils": "^2.4.6",
    "cz-git": "^1.9.3",
    "czg": "^1.9.3",
    "eslint": "^8.57.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "pnpm": "^8.15.8",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.10.3",
    "typescript": "^5.4.5",
    "unocss": "^0.53.6",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.3",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
